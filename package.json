{
  "name": "@outcome-co/verdaccio-google-cloud",
  "version": "1.1.3",
  "description": "Enhancements to the Verdaccio Google Cloud storage plugin",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel src -d dist --copy-files",
    "clean": "rm -rf dist coverage",
    "lint": "eslint --ignore-path .gitignore --max-warnings 0 ./src",
    "postbuild": "gulp postbuild",
    "prebuild": "gulp clean",
    "prepublishOnly": "yarn build",
    "pretest": "yarn typecheck",
    "server": "npm run build && nodemon --delay 1 --watch dist --signal SIGTERM ./run/server.js",
    "test": "yarn test:unit",
    "test:unit": "jest --passWithNoTests ./src",
    "type-check": "tsc",
    "typecheck": "tsc",
    "watch": "watch 'yarn build' src --wait=0.5",
    "watch:push": "watch 'yalc push --changed' src --wait=0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/outcome-co/verdaccio-google-cloud-js"
  },
  "author": {
    "name": "Outcome Engineering",
    "email": "engineering@outcome.co",
    "url": "https://outcome.co"
  },
  "license": "ISC",
  "bugs": {
    "email": "engineering@outcome.co"
  },
  "homepage": "https://github.com/outcome-co/verdaccio-google-cloud-js",
  "dependencies": {
    "lodash": "^4.17.20",
    "verdaccio-google-cloud": "^9.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@outcome-co/devkit": "^9.0.2",
    "@types/gulp": "^4.0.7",
    "@types/gulp-sourcemaps": "^0.0.33",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "@verdaccio/types": "^9.7.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsdoc": "^30.7.7",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5",
    "verdaccio": "^4.8.1",
    "watch": "^1.0.2"
  },
  "engines": {
    "node": ">=10"
  },
  "optionalDependencies": {
    "@outcome-co/verdaccio-github-auth": "^1.1.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "chokidar": "^3.4.0",
    "node-forge": "^0.10.0",
    "dompurify": "^2.0.17"
  }
}
